{
    "manifest_version": 2,
    "name": "MooCification",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs","storage","http://*/","https://*/"],
    "browser_action":{
      "default_popup": "popup/login.html",
      "default_title": "MooCification"
    }
}